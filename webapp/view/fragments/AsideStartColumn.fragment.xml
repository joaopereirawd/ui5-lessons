<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Table id="list"
		items="{path: 'ModelClients>/results'}"
		itemPress="test"
		>

		<!-- Toolbar -->
		<infoToolbar>	
			<OverflowToolbar>
	      		<Label id="tableHeader" text="{worklistView>/worklistTableTitle}"/>
                <ToolbarSpacer />
                <SearchField id="searchField" width="auto" liveChange="onLiveSearch" />
				<ToolbarSpacer />
			</OverflowToolbar>
		</infoToolbar>

		<!-- Columns -->
		<columns>
			<Column
				width="12em">
				<Text text="Projects" />
			</Column>
		</columns>

		<!-- Items -->
		<items>
			<ColumnListItem
			type="Navigation" press="onClickHandler">
				<cells>
					<ObjectIdentifier title="{ModelClients>Nomecliente}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>